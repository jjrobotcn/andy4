{
  "swagger": "2.0",
  "info": {
    "title": "power.proto",
    "version": "version not set"
  },
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/api/v2/power/get_power_status": {
      "get": {
        "summary": "获取电源状态数据流",
        "operationId": "GetPowerStatus",
        "responses": {
          "200": {
            "description": "A successful response.(streaming responses)",
            "schema": {
              "$ref": "#/x-stream-definitions/powerServiceGetPowerStatusResponse"
            }
          }
        },
        "tags": [
          "PowerService"
        ]
      }
    },
    "/api/v2/power/reboot": {
      "post": {
        "summary": "对各模块的电源进行断电方式重启",
        "operationId": "Reboot",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/powerServiceRebootResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/powerServiceRebootRequest"
            }
          }
        ],
        "tags": [
          "PowerService"
        ]
      }
    },
    "/api/v2/power/states": {
      "get": {
        "summary": "获取所有电源模块状态",
        "operationId": "States",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/powerServiceStatesResponse"
            }
          }
        },
        "tags": [
          "PowerService"
        ]
      }
    },
    "/api/v2/power/switch": {
      "post": {
        "summary": "控制模块供电开关",
        "operationId": "Switch",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/powerServiceSwitchResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/powerServiceSwitchRequest"
            }
          }
        ],
        "tags": [
          "PowerService"
        ]
      }
    }
  },
  "definitions": {
    "SwitchRequestrequest": {
      "type": "object",
      "properties": {
        "module": {
          "$ref": "#/definitions/powerServiceModuleTypes",
          "title": "模块类型"
        },
        "off_after": {
          "type": "integer",
          "format": "int32",
          "title": "若干秒后关闭该模块供电\n-1为未配置 0为立即"
        },
        "on_after": {
          "type": "integer",
          "format": "int32",
          "title": "若干秒后恢复该模块供电\n-1为未配置 0为立即"
        }
      }
    },
    "powerServiceGetPowerStatusResponse": {
      "type": "object",
      "properties": {
        "power_status": {
          "$ref": "#/definitions/powerServicePowerStatus"
        }
      }
    },
    "powerServiceModuleTypes": {
      "type": "string",
      "enum": [
        "UnknownModuleType",
        "Main",
        "EscPos",
        "Screen",
        "Sensor",
        "Speech",
        "Lights",
        "Expression",
        "Navigator",
        "Motion",
        "Amplifier"
      ],
      "default": "UnknownModuleType",
      "title": "模块类型枚举"
    },
    "powerServicePowerStatus": {
      "type": "object",
      "properties": {
        "level": {
          "type": "integer",
          "format": "int64"
        },
        "is_charging": {
          "type": "boolean",
          "format": "boolean"
        },
        "devices": {
          "type": "object",
          "additionalProperties": {
            "type": "boolean",
            "format": "boolean"
          },
          "title": "设备状态\n此字段已作废，请使用States方法获取"
        }
      }
    },
    "powerServiceRebootRequest": {
      "type": "object",
      "properties": {
        "all": {
          "type": "boolean",
          "format": "boolean",
          "title": "所有模块重启(目前仅支持整机重启)"
        }
      }
    },
    "powerServiceRebootResponse": {
      "type": "object"
    },
    "powerServiceState": {
      "type": "object",
      "properties": {
        "module": {
          "$ref": "#/definitions/powerServiceModuleTypes",
          "title": "模块类型"
        },
        "is_on": {
          "type": "boolean",
          "format": "boolean",
          "title": "当前模块是否供电"
        },
        "off_after": {
          "type": "integer",
          "format": "int32",
          "title": "触发关闭供电倒计时（秒）\n-1为未配置或已失效"
        },
        "on_after": {
          "type": "integer",
          "format": "int32",
          "title": "触发恢复供电倒计时（秒）\n-1为未配置或已失效"
        }
      }
    },
    "powerServiceStatesResponse": {
      "type": "object",
      "properties": {
        "states": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/powerServiceState"
          }
        }
      }
    },
    "powerServiceSwitchRequest": {
      "type": "object",
      "properties": {
        "requests": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/SwitchRequestrequest"
          }
        }
      }
    },
    "powerServiceSwitchResponse": {
      "type": "object",
      "properties": {
        "states": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/powerServiceState"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "runtimeStreamError": {
      "type": "object",
      "properties": {
        "grpc_code": {
          "type": "integer",
          "format": "int32"
        },
        "http_code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "http_status": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  },
  "x-stream-definitions": {
    "powerServiceGetPowerStatusResponse": {
      "type": "object",
      "properties": {
        "result": {
          "$ref": "#/definitions/powerServiceGetPowerStatusResponse"
        },
        "error": {
          "$ref": "#/definitions/runtimeStreamError"
        }
      },
      "title": "Stream result of powerServiceGetPowerStatusResponse"
    }
  }
}
