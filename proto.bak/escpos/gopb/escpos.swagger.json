  "definitions": {
    "escposServiceDeleteHtmlTemplatesRequest": {
      "type": "object",
      "properties": {
        "html_templates_id": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "title": "删除模板id列表"
        }
      },
      "title": "删除html模板请求"
    },
    "escposServiceDeleteHtmlTemplatesResponse": {
      "type": "object",
      "title": "删除html模板结果"
    },
    "escposServiceElement": {
      "type": "object",
      "properties": {
        "etype": {
          "$ref": "#/definitions/escposServiceElementType",
          "title": "打印类型"
        },
        "content_text": {
          "type": "string",
          "title": "文本类型打印内容"
        },
        "content_image": {
          "type": "string",
          "format": "byte",
          "title": "图片类型打印内容"
        },
        "image_content_type": {
          "type": "string",
          "title": "图片类型， 默认为png (png|jpg|jpeg|bmp)"
        },
        "size": {
          "$ref": "#/definitions/escposServiceElementSize",
          "title": "字号\n图片类型无效"
        }
      },
      "title": "打印对象"
    },
    "escposServiceElementAlign": {
      "type": "string",
      "enum": [
        "LEFT",
        "CENTER",
        "RIGHT"
      ],
      "default": "LEFT",
      "description": "- LEFT: 左对齐\n - CENTER: 居中\n - RIGHT: 右对齐",
      "title": "行对齐方式"
    },
    "escposServiceElementRow": {
      "type": "object",
      "properties": {
        "elements": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/escposServiceElement"
          },
          "title": "打印对象列表"
        },
        "align": {
          "$ref": "#/definitions/escposServiceElementAlign",
          "title": "行对齐方式"
        }
      },
      "title": "单行中打印对象"
    },
    "escposServiceElementSize": {
      "type": "string",
      "enum": [
        "DEFAULT",
        "SMALL",
        "NORMAL",
        "LARGE",
        "HUGE"
      ],
      "default": "DEFAULT",
      "description": "- DEFAULT: DEFAULT = NORMAL\n - SMALL: 小字体\n - NORMAL: 中字体\n - LARGE: 大字体\n - HUGE: 超大字体",
      "title": "文字尺寸"
    },
    "escposServiceElementType": {
      "type": "string",
      "enum": [
        "TEXT",
        "IMAGE"
      ],
      "default": "TEXT",
      "description": "- TEXT: 文本类型\n - IMAGE: 图片类型",
      "title": "内容类型"
    },
    "escposServiceGetHtmlTemplateRequest": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "title": "html模板id"
        }
      },
      "title": "获取指定模板内容请求"
    },
    "escposServiceGetHtmlTemplateResponse": {
      "type": "object",
      "properties": {
        "html_template": {
          "$ref": "#/definitions/escposServiceHtmlTemplate",
          "title": "html模板"
        }
      },
      "title": "获取指定模板内容结果"
    },
    "escposServiceHtmlTemplate": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "title": "创建时id将自动生成\n修改时根据此id获取模板数据"
        },
        "name": {
          "type": "string",
          "title": "创建时传入空值将生成随机名\n修改时传入空值将不改变"
        },
        "content": {
          "type": "string",
          "title": "修改时传入空值将不改变"
        }
      },
      "title": "html模板"
    },
    "escposServiceListHtmlTemplatesRequest": {
      "type": "object",
      "title": "获取模板列表请求"
    },
    "escposServiceListHtmlTemplatesResponse": {
      "type": "object",
      "properties": {
        "html_templates": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/escposServiceHtmlTemplate"
          },
          "title": "html模板列表"
        }
      },
      "title": "获取模板列表结果"
    },
    "escposServiceNewHtmlTemplateRequest": {
      "type": "object",
      "properties": {
        "html_template": {
          "$ref": "#/definitions/escposServiceHtmlTemplate",
          "title": "html模板"
        }
      },
      "title": "创建html模板请求"
    },
    "escposServiceNewHtmlTemplateResponse": {
      "type": "object",
      "properties": {
        "html_template": {
          "$ref": "#/definitions/escposServiceHtmlTemplate",
          "title": "html模板"
        }
      },
      "title": "创建html模板结果"
    },
    "escposServicePrintElementsRequest": {
      "type": "object",
      "properties": {
        "element_rows": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/escposServiceElementRow"
          },
          "title": "打印element行对象列表"
        },
        "cut": {
          "type": "boolean",
          "format": "boolean",
          "title": "是否切纸 需打印机类型支持"
        }
      },
      "title": "打印对象请求"
    },
    "escposServicePrintElementsResponse": {
      "type": "object",
      "title": "打印对象结果"
    },
    "escposServicePrintHtmlTemplateRequest": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "title": "可与name选填"
        },
        "name": {
          "type": "string",
          "title": "可与id选填, 仅匹配首个同名模板"
        },
        "params": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "title": "模板参数, 模板页面中使用\"{{}}\"符号标记\n如: 联系电话: {{tel}}, 参数中使用{\"params\": {\"tel\": \"123\"}}\n打印内容则为:  联系电话: 123"
        },
        "cut": {
          "type": "boolean",
          "format": "boolean",
          "title": "是否切纸 需打印机类型支持"
        }
      },
      "title": "html模板打印请求"
    },
    "escposServicePrintHtmlTemplateResponse": {
      "type": "object",
      "title": "html模板打印结果"
    },
    "escposServicePrintRawRequest": {
      "type": "object",
      "properties": {
        "raw": {
          "type": "string",
          "format": "byte",
          "title": "escpos协议数据"
        }
      },
      "title": "打印bytes数据请求"
    },
    "escposServicePrintRawResponse": {
      "type": "object",
      "title": "打印bytes数据结果"
    },
    "escposServiceUpdateHtmlTemplateRequest": {
      "type": "object",
      "properties": {
        "html_template": {
          "$ref": "#/definitions/escposServiceHtmlTemplate",
          "title": "html模板"
        }
      },
      "title": "更新html模板请求"
    },
    "escposServiceUpdateHtmlTemplateResponse": {
      "type": "object",
      "properties": {
        "html_template": {
          "$ref": "#/definitions/escposServiceHtmlTemplate",
          "title": "html模板"
        }
      },
      "title": "更新html模板结果"
    }
  }
}
